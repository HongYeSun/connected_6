{
	"id": "com.app.enact-template",
	"version": "1.0.0",
	"vendor": "LG Electronics, Inc.",
	"type": "web",
	"main": "index.html",
	"title": "BeanBox",
	"accessibility": {"supportsAudioGuidance": true},
	"allowAudioCapture": false,
	"allowVideoCapture": false,
	"disableBackHistoryAPI": true,
	"handleScreenRemoteKey": true,
	"icon": "icon.png",
	"largeIcon": "icon.png",
	"lockable": false,
	"noSplashOnLaunch": true,
	"resolution": "1920x1080",
	"resolutionIndependent": false,
	"supportPortraitMode": true,
	"supportTouchMode": "full",
	"transparent": false,
	"uiRevision": 2,
	"useCamera": false,
	"useGraphicCamera": false,
	"useNativeScroll": true,
	"visible": true,
	"requiredPermissions": [
		"time.query",
		"activity.operation",
		"notification.operation",
		"memorymanager.query",
		"memorymanger.management",
		"sdkagent.collector"
	]
}
